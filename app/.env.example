# App Environment Configuration

# API Configuration
EXPO_PUBLIC_API_URL=http://localhost:8080
EXPO_PUBLIC_AI_SERVICE_URL=http://localhost:8000
EXPO_PUBLIC_WS_URL=ws://localhost:8080/ws

# Environment
EXPO_PUBLIC_ENV=development
NODE_ENV=development

# Feature Flags
EXPO_PUBLIC_ENABLE_AI_FEATURES=true
EXPO_PUBLIC_ENABLE_OFFLINE_MODE=true
EXPO_PUBLIC_ENABLE_ANALYTICS=false

# App Configuration
EXPO_PUBLIC_APP_NAME=Construction Estimator
EXPO_PUBLIC_APP_VERSION=1.0.0

# Authentication
EXPO_PUBLIC_AUTH_DOMAIN=localhost
EXPO_PUBLIC_AUTH_TIMEOUT=3600000

# Logging
EXPO_PUBLIC_LOG_LEVEL=debug
EXPO_PUBLIC_ENABLE_ERROR_REPORTING=false

# Map Services (replace with actual keys)
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=your-google-maps-api-key
EXPO_PUBLIC_MAPBOX_TOKEN=your-mapbox-token

# Analytics (replace with actual keys if enabled)
EXPO_PUBLIC_ANALYTICS_ID=
EXPO_PUBLIC_SENTRY_DSN=
